    <script>
        var animals = ["Tomato" , "Salad" , "Egg" , "Fish"]
        var L
        yes = ""
        for (let L = 0; L < animals.length; L++) {
            yes =  yes + animals [L] + "<br>";
            
        }
        document.getElementById("res").innerHTML = yes
        </script>