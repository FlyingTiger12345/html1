    <script>
        var a = [12,67,34,41,69]
        function maper(a){
            return a*3
        }
        ar = a.map(maper)
        document.write(a + "<br>")
       
        document.write(ar + "<br>")
        function up(){
            ar.sort(function(a,b){return a - b})
            document.write(ar)
        }
        up()
        </script>