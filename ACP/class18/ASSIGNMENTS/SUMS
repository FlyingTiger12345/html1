    <script>

function sumimum() {

    var sum = 0;
    for (var i = 0; i < arguments.length; i++) {
        sum += arguments[i];
    }
    return sum;
}

document.getElementById("res").innerHTML =
  "The sum value is " + sumimum(10, 20, 30, 40, 50, 60, 70, 80);
  

</script>