    <body>
        <div id="container">
            <div id="res"></div>
        <div id="res1"></div>
        <div id="res2"></div>
        <div id="res3"></div>
        </div>
    </body>
    <script>

function maximum() {
    var i, max = arguments[0];
    for (i = 1; i < arguments.length; i++) {
        if (arguments[i] > max) {
            max = arguments[i];
        }
    }
    return max;
}

document.getElementById("res").innerHTML = "The max value is " + maximum(10, 20, 30, 40, 50, 60, 70, 8);

</script>