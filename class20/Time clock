    <script>
   var yes = new Date()
   var no = yes.getHours()

   if (no<10)

   {weather = "Good Morning"
    }
else if (no<20)
{weather = "Good day"
    }
    else{
        weather = "Good night"
    }
    document.getElementById("res1").innerHTML = "time of day is: " + (no)  +  weather
    </script>
