    <script>
        var wale =  {"title":"king", "age":12}
        var jason = JSON.stringify(wale)
    document.getElementById("res").innerHTML = "Jason before: " + jason
        var jason2 = JSON.parse(jason)
        document.getElementById("res1").innerHTML = "Jason after: " + jason2.title + "<br>" + jason2.age






        </script>