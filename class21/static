    <script>
        class myclass{
            method1(){
                return "I am the one who knocks"
            }
            static am(){
            return "I am the one who knocks1"
            }
            }
            ob = new myclass()
            document.getElementById("res1").innerHTML = ob.method1()
            document.getElementById("res").innerHTML = myclass.am()
            </script>
